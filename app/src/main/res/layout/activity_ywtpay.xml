<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    android:orientation="vertical"
    tools:context="com.example.huichuanyi.ui.activity.pay.YWTPayActivity">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:background="@color/activityTop">

        <include layout="@layout/btn_back"></include>

        <TextView
            style="@style/ActivityTopTextTitleStyle"
            android:layout_centerInParent="true"
            android:text="一网通支付" />

    </RelativeLayout>

    <com.example.huichuanyi.ui.activity.custom.YWTWebView
        android:id="@+id/ywt_pay"
        android:layout_width="match_parent"
        android:layout_height="match_parent"></com.example.huichuanyi.ui.activity.custom.YWTWebView>
</LinearLayout>
