<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/hm_background"
    android:orientation="vertical">

    <TextView
        android:id="@+id/item_see_car_address"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/distance_5"
        android:text="地址"
        android:textSize="@dimen/text_size_16" />

    <TextView
        android:id="@+id/item_see_car_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/distance_5"
        android:layout_marginLeft="@dimen/distance_5"
        android:layout_marginTop="@dimen/distance_5"
        android:text="时间"
        android:textSize="@dimen/text_size_16" />

</LinearLayout>